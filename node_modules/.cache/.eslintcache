seState, useEffect } from 'react'\nimport { FiShoppingCart } from 'react-icons/fi'\nimport { GrClose } from 'react-icons/gr'\nimport CartItems from './CartItems'\nimport './Cart.css'\n\nconst Cart = ({MenuData, RemoveItem, addOrder}) => {\n    if(MenuData.length === 0 ) {\n        // console.log(\"Im from Cart.js, MenuData === 0\")\n    }\n\n    const hideStyle = {\n        display: \"none\"\n    }\n    const unhideStyle = {\n         // TODO: add some styling\n    }\n    \n    const [counter, setCounter] = useState(0)\n    const [total, setTotal] = useState(0)\n    const [showCart, setShowCart] = useState(false)\n    const [showCheckout, setShowCheckout] = useState(false)\n    const [toggleExpandCart, setToggleExpandCart] = useState(hideStyle)\n    const [toggleCheckout, setToggleCheckout] = useState(hideStyle)\n    const [toggleCart, setToggleCart] = useState(unhideStyle)\n    const [cartTitle, setCartTitle] = useState('Your Cart')\n    const [orderName, setOrderName] = useState('')\n    const [orderTel, setOrderTel] = useState('')\n    const [orderEmail, setOrderEmail] = useState('')\n\n    useEffect(() => {\n        // console.log(MenuData)\n        let count = 0\n        let bill = 0\n        MenuData.forEach( (key, value)=> {\n            // console.log('key.key.price: ' + key.key.price + ' value: ' + key.value)\n            count = count + key.value\n            bill = bill + (key.key.price * key.value)\n        })\n        // console.log('bill: ' + bill + ' count: ' + count)\n        setCounter(count)\n        setTotal(bill)\n    })\n\n    const handleCartClick = () => {\n        if(showCart) {\n            setToggleExpandCart(hideStyle)\n            setShowCart(!showCart)\n        } else {\n            setToggleExpandCart(unhideStyle)\n            setShowCart(!showCart)\n        }\n    }\n\n    const handleCheckoutClick = () => {\n        if(!showCheckout) {\n            setShowCheckout(!showCheckout)\n            setToggleCheckout(unhideStyle)\n            setToggleCart(hideStyle)\n            setCartTitle('Checkout')\n        } else {\n            setShowCheckout(!showCheckout)\n            setToggleCheckout(hideStyle)\n            setToggleCart(unhideStyle)\n            setCartTitle('Your Cart')\n        }\n    }\n\n    const handleSubmit = (event) => {\n        event.preventDefault()\n        // TODO: post to database\n        // alert('Order has been submitted')\n        let order = { name: orderName, telephone: orderTel, email: orderEmail, order: MenuData}\n        // console.log(JSON.stringify(order))\n        addOrder(order)\n\n    }\n\n    return (\n        <div className='cart-wrapper'>\n            <button onClick={handleCartClick} className='icon-container'>\n                <FiShoppingCart/>\n                <div className='counter'>\n                    {counter}\n                </div>\n            </button>\n            <div className='expand-cart' style={toggleExpandCart}>\n                <header className='cart-header-wrapper'>\n                    <h1 className='cart-header'>{cartTitle}</h1>\n                    <button onClick={handleCartClick} className='close-cart'><GrClose /></button>\n                </header>\n                <div className='cart-wrapper' style={toggleCart}>\n                    <CartItems items={MenuData} remove={RemoveItem}/>\n                    <h2>{total.toFixed(2)}</h2>\n                    <button onClick={handleCheckoutClick} className='checkout-btn'>checkout</button>\n                </div>\n                <div className='checkout-wrapper' style={toggleCheckout}>\n                    <form onSubmit={handleSubmit}>\n                        <label>\n                            <input\n                            type=\"text\"\n                            placeholder=\"Name\"\n                            value={orderName}\n                            onChange={ (e) => setOrderName(e.target.value)}\n                            />                            \n                        </label>\n                        <label>\n                            <input\n                            placeholder=\"Telephone\"\n                            type=\"text\" value={orderTel}\n                            onChange={ (e) => setOrderTel(e.target.value)}\n                            />                            \n                        </label>\n                        <label>\n                            <input\n                            placeholder=\"email\"\n                            type=\"text\"\n                            value={orderEmail}\n                            onChange={ (e) => setOrderEmail(e.target.value)}\n                            />                            \n                        </label>\n                        <input type=\"submit\" />\n                    </form>\n                    <button onClick={handleCheckoutClick} className='back-btn'>back</button>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default Cart;","/Users/david/Desktop/Projects/HungryHippoRestaurant/src/components/menu/CartItem.js",[],"/Users/david/Desktop/Projects/HungryHippoRestaurant/src/components/menu/CartItems.js",[],{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":29,"column":11,"nodeType":"59","messageId":"60","endLine":29,"endColumn":14},{"ruleId":"61","severity":1,"message":"62","line":43,"column":7,"nodeType":"63","messageId":"64","endLine":43,"endColumn":8},{"ruleId":"65","severity":1,"message":"66","line":31,"column":5,"nodeType":"59","endLine":31,"endColumn":14,"suggestions":"67"},"no-native-reassign",["68"],"no-negated-in-lhs",["69"],"no-unused-vars","'res' is assigned a value but never used.","Identifier","unusedVar","no-sequences","Unexpected use of comma operator.","SequenceExpression","unexpectedCommaExpression","react-hooks/exhaustive-deps","React Hook useEffect contains a call to 'setCounter'. Without a list of